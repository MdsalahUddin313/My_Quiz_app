<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.myquizapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Technology.Technologies_qsn"></activity>
        <activity android:name=".Football_part.ResultActivity" />
        <activity android:name=".Football_part.fquiz" />
        <activity
            android:name=".Football_part.Football"
            android:parentActivityName=".Dashboard"
            android:theme="@style/NoAction_bar" />
        <activity
            android:name=".Dashboard"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>