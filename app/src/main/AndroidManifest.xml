<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.myquizapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Authintication.shared_profile"
            android:theme="@style/NoAction_bar"></activity>
        <activity android:name=".Authintication.Admin_profile"
            android:theme="@style/NoAction_bar"/>
        <activity
            android:name=".Authintication.Register_activity"
            android:theme="@style/NoAction_bar" />
        <activity
            android:name=".Authintication.LoginActivity"
            android:theme="@style/NoAction_bar" />
        <activity android:name=".Question_And_Answer.StartingScreenActivity" />
        <activity android:name=".Question_And_Answer.Quiz" />
        <activity android:name=".Technology.Technologies_qsn" />
        <activity
            android:name=".Dashboard"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>